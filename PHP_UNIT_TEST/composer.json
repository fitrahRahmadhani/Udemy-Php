{
  "name": "fitrahrahmadhani/belajar-php-unit-test",
  "description": "Belajar PHP Unit Test",
  "type": "project",
  "authors": [
    {
      "name": "fitrahRahmadhani",
      "email": "2131710092@student.polinema.ac.id"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\Test\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --configuration phpunit.xml",
    "test:product": "phpunit --configuration phpunit.xml --testsuite product"
  }
}
